<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation=
                 "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <display-name>mainServleta</display-name>
  <description>
    This is a simple web application which calculate
    index of wages.
  </description>

  <servlet>
    <servlet-name>mainn</servlet-name>
    <servlet-class>xyz.dimonick.Servlet.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>res</servlet-name>
    <servlet-class>xyz.dimonick.Servlet.ResultsServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>mainn</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>res</servlet-name>
    <url-pattern>/result</url-pattern>
  </servlet-mapping>

</web-app>
